{
  "schemaVersion": 1,
  "id": "fwaystones",
  "version": "${version}",
  "name": "Fabric Waystones",
  "description": "A better way of transport!",
  "authors": [
    "LordDeatHunter"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/fabric-waystones",
    "sources": "https://github.com/LordDeatHunter/FabricWaystones",
    "issues": "https://github.com/LordDeatHunter/FabricWaystones/issues"
  },
  "license": "MIT License",
  "icon": "assets/fwaystones/fabric_waystones_icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "wraith.fwaystones.FabricWaystones"
    ],
    "client": [
      "wraith.fwaystones.client.WaystonesClient"
    ],
    "journeymap": [
      "wraith.fwaystones.integration.journeymap.JourneymapWaypointMaker"
    ]
  },
  "mixins": [
    "fwaystones.mixins.json"
  ],
  "accessWidener": "fwaystones.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric": "*",
    "minecraft": "~1.21",
    "java": ">=21",
    "owo": ">=0.10.3"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_269": [
        "net/fabricmc/fabric/api/attachment/v1/AttachmentTarget"
      ]
    }
  }
}
